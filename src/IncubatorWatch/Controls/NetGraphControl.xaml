<UserControl x:Class="IncubatorWatch.Controls.NetGraphControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0" mc:Ignorable="d"
             d:DesignHeight="308" d:DesignWidth="495" Name="NetMeterGadget">
    <Grid>
        <d3:ChartPlotter  Name="plotter" Margin="10,10,10,10">
            <d3:ChartPlotter.HorizontalAxis>
                <d3:HorizontalDateTimeAxis Name="timeAxis" Visibility="Hidden" />
            </d3:ChartPlotter.HorizontalAxis>
            <d3:ChartPlotter.VerticalAxis>
                <d3:VerticalAxis Name="countAxis" Visibility="Visible" />
            </d3:ChartPlotter.VerticalAxis>
            <d3:HorizontalAxisTitle  Name="HorizontalAxisName"  Content="" FontSize="8" />
            <d3:VerticalAxisTitle Content="Y-Axis (default axis)"/>
        </d3:ChartPlotter>
        
        <Label Content="60 Secondes" FontSize="11" Height="26" HorizontalAlignment="Right" HorizontalContentAlignment="Right" Margin="0,0,21,32" Name="label3" VerticalAlignment="Bottom" VerticalContentAlignment="Center" Width="98" />
    </Grid>
</UserControl>