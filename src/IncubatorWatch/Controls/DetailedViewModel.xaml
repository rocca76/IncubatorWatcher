<UserControl x:Class="IncubatorWatch.Controls.DetailedViewModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Controls1="clr-namespace:IncubatorWatch.Controls"
             mc:Ignorable="d"
             d:DesignHeight="498" d:DesignWidth="683"
             DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid Height="490">
        <TabControl Name="tabCtrlMother">
            <TabItem Header="Statistics">
                <Grid>
                    <Label Content="Température :" Height="28" HorizontalAlignment="Left" Margin="45,42,0,0" Name="label7" VerticalAlignment="Top" />
                    <Label Content="Humidité Relative :" Height="28" HorizontalAlignment="Left" Margin="45,76,0,0" Name="label8" VerticalAlignment="Top" Width="134" />
                    <Label Content="0" Height="28" HorizontalAlignment="Left" Margin="175,42,0,0" Name="lbl_TotalRcvd" VerticalAlignment="Top" Width="295" />
                    <Label Content="0" Height="28" HorizontalAlignment="Left" Margin="175,76,0,0" Name="lbl_TotalSent" VerticalAlignment="Top" Width="295" />
                </Grid>
            </TabItem>
            <TabItem Header="Graphique">
                <StackPanel Name="stackPanel1" Height="461">
                    <Controls1:NetGraphControl x:Name="netGraphControl1" />
                    <Controls1:NetGraphControl x:Name="netGraphControl2" />
                </StackPanel>
            </TabItem>
            <TabItem Header="About">
                <Grid>
                    <Label Content="Incubator Watcher" Height="36" HorizontalAlignment="Left" Margin="10,10,0,0" Name="label1" VerticalAlignment="Top" FontSize="18" />
                    <Label Content="BETA" Height="36" HorizontalAlignment="Left" Margin="159,10,0,0" x:Name="label1_Copy" VerticalAlignment="Top" FontSize="12" />
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>